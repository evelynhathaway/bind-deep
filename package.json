{
  "name": "bind-deep",
  "version": "2.2.3",
  "description": "Lightweight module for binding a function or object deeply.",
  "keywords": [
    "bind",
    "context",
    "deep",
    "function",
    "object",
    "this"
  ],
  "bugs": {
    "url": "https://github.com/evelynhathaway/bind-deep/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evelynhathaway/bind-deep.git"
  },
  "license": "MIT",
  "author": "Evelyn Hathaway <npm@evelyn.dev> (https://evelyn.dev)",
  "main": "./lib/index.js",
  "types": "./index.d.ts",
  "typesVersions": {
    ">=4.1": {
      "*": [
        "./types-versions/ts4.1/*"
      ]
    }
  },
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "files": [
    "/lib",
    "/types-versions",
    "/index.d.ts"
  ],
  "scripts": {
    "build": "babel src -d lib",
    "lint": "eslint ./",
    "prepare": "husky install",
    "test": "mocha test",
    "test:watch": "mocha test -w"
  },
  "commitlint": {
    "extends": "./node_modules/conventional-changelog-evelyn/commitlint.config.js"
  },
  "lint-staged": {
    "**/package.json": "sort-package-json",
    "./": "eslint --fix"
  },
  "release": {
    "extends": "conventional-changelog-evelyn/release.config.js"
  },
  "dependencies": {
    "ts-toolbelt": "^9.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "chai": "^4.3.6",
    "commitlint": "^17.0.3",
    "conventional-changelog-evelyn": "^1.3.1",
    "eslint": "^8.22.0",
    "eslint-plugin-evelyn": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "semantic-release": "^19.0.3",
    "sort-package-json": "^1.57.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
